! UI
urxvt.iconFile:    /usr/share/icons/Faenza/apps/16/terminal.png
urxvt.scrollstyle: plain
urxvt.scrollBar: false
urxvt.visualBell:  true

! Extensions
urxvt.perl-ext-common: default,matcher,-option-popup,-selection-popup
urxvt.urlLauncher:     /usr/bin/google-chrome
urxvt.matcher.button:  1
urxvt.colorUL: #4682B4

! Fonts
urxvt.font:    xft:Tamsyn:size=10:antialias=false

! Theme
